{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "window": true,
    "document": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "linebreak-style": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "comma-dangle": 0,
    "no-confusing-arrow": 0,
    "no-shadow": 0,
    "no-undef": 0,
    "react/forbid-prop-types": 0,
    "semi": 0
  }
}
